    <TargetFramework>net9.0</TargetFramework>
    <RootNamespace>nullboard_agent_master</RootNamespace>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
  </PropertyGroup>

  <PropertyGroup>
    <Description>NullBoardAgent is a web api that nullboard.io calls to back up the boards</Description> 
